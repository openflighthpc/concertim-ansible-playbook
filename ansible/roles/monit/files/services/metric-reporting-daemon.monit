check process metric-reporting-daemon with matching "metric-reporting-daemon"
  start program = "/usr/bin/systemctl restart metric-reporting-daemon" with timeout 60 seconds
  restart program = "/usr/bin/systemctl restart metric-reporting-daemon" with timeout 60 seconds
  stop program = "/usr/bin/systemctl stop metric-reporting-daemon"
  group ct
  group ct-metrics
  group ct-metric-reporting

check file metric-reporting-daemon_log with path /var/log/ct-metric-reporting-daemon.log
  start program = "/usr/bin/systemctl restart metric-reporting-daemon" with timeout 60 seconds
  restart program = "/usr/bin/systemctl restart metric-reporting-daemon" with timeout 60 seconds
  stop program = "/usr/bin/systemctl stop metric-reporting-daemon"
  if timestamp > 5 minutes then restart
  group ct
  group ct-metrics
  group ct-metric-reporting
