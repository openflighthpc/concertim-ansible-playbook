PORTThresholdsTable	OBJECT-TYPE
    SYNTAX			SEQUENCE OF PORTThresholdsEntry
    MAX-ACCESS  		not-accessible
    STATUS  		current
    DESCRIPTION   	""
    
    ::= { TRAPThresholds 17 }

PORTThresholdsEntry	OBJECT-TYPE
    SYNTAX			PORTThresholdsEntry
    MAX-ACCESS		not-accessible
    STATUS			current
    DESCRIPTION   	"Table of Port trap notification thresholds."
    INDEX			{ PORTThresholdsNumber }    
    
    ::= { PORTThresholdsTable 1 }

PORTThresholdsEntry ::= SEQUENCE {

	  PORTThresholdsNumber
		INTEGER,
	  PORTAMPSLowAlarm
		INTEGER,
	  PORTAMPSLow
		INTEGER,
	  PORTAMPSHighAlarm
		INTEGER,
	  PORTAMPSHigh
		INTEGER,
  }

PORTThresholdsNumber	OBJECT-TYPE
    SYNTAX			INTEGER (1..32)
    MAX-ACCESS  		not-accessible
    STATUS  		current
    DESCRIPTION   	"Trap threshold PORT number [1..32]."
    
    ::= { PORTThresholdsEntry 1 }

PORTAMPSLowAlarm		OBJECT-TYPE
    SYNTAX		INTEGER {DISABLED(0),ENABLED(1)}
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Port Current LOW alarm disabled/enabled."
    
    ::= { PORTThresholdsEntry 2 }

PORTAMPSLow	OBJECT-TYPE
    SYNTAX		INTEGER
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Port Current LOW trap threshold x10."
    
    ::= { PORTThresholdsEntry 3 }

PORTAMPSHighAlarm		OBJECT-TYPE
    SYNTAX		INTEGER {DISABLED(0),ENABLED(1)}
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Port Current HIGH alarm disabled/enabled."
    
    ::= { PORTThresholdsEntry 4 }

PORTAMPSHigh	OBJECT-TYPE
    SYNTAX		INTEGER
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Port Current HIGH trap threshold x10."
    
    ::= { PORTThresholdsEntry 5 }

