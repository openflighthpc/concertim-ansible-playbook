-- PDU.PORTStatus

PORTStatusTable	OBJECT-TYPE
    SYNTAX		SEQUENCE OF PDUStatusEntry
    MAX-ACCESS  	not-accessible
    STATUS  	current
    DESCRIPTION   ""
    
    ::= { PORTStatus 1 }
    
PORTStatusEntry	OBJECT-TYPE
    SYNTAX		PDUStatusEntry
    MAX-ACCESS	not-accessible
    STATUS		current
    DESCRIPTION   "Table of PORT status parameters."
    INDEX		{ PORTIndex }    
    
    ::= { PORTStatusTable 1 }

PDUStatusEntry ::= SEQUENCE {

	  PORTIndex
		INTEGER,
	  PORTNumber
		INTEGER,
	  PORTName
		DisplayString,
	  PORTONOFF
		INTEGER,
	  PORTAMPS
		INTEGER,
	  PORTKWH
		Integer32,
  }

PORTIndex		OBJECT-TYPE
    SYNTAX		INTEGER
    MAX-ACCESS  	not-accessible
    STATUS  	current
    DESCRIPTION   "Port index"
    
    ::= { PORTStatusEntry 1 }

PORTNumber		OBJECT-TYPE
    SYNTAX		INTEGER
    MAX-ACCESS  	not-accessible
    STATUS  	current
    DESCRIPTION   "Port number [Range 1 to PDUNoPORTs]"
    
    ::= { PORTStatusEntry 2 }

PORTName		OBJECT-TYPE
    SYNTAX		DisplayString (SIZE (0..10))
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Port friendly name."
    
    ::= { PORTStatusEntry 3 }

PORTONOFF		OBJECT-TYPE
    SYNTAX		INTEGER {OFF(0),ON(1)}
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Port ON/OFF status [Set to 1 to switch ON, 0 to switch OFF]"
    
    ::= { PORTStatusEntry 4 }

PORTAMPS		OBJECT-TYPE
    SYNTAX		INTEGER
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "Port current (Amps) x10."
    
    ::= { PORTStatusEntry 5 }

PORTKWH		OBJECT-TYPE
    SYNTAX		Integer32
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Port Energy (kWh) x10."
    
    ::= { PORTStatusEntry 6 }

PORTSelected	OBJECT-TYPE
    SYNTAX		INTEGER (1..32)
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Currently selected PORT. Set to number between 1 and PDUNoPORTs"
    
    ::= { PORTStatus 2 }    

