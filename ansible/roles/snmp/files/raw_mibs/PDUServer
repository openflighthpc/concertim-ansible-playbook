-- PDUTraps.TRAPThresholds

PDUVRMSLowAlarm		OBJECT-TYPE
    SYNTAX		INTEGER {DISABLED(0),ENABLED(1)}
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "PDU Voltage LOW alarm disabled/enabled."
    
    ::= { TRAPThresholds 1 }

PDUVRMSLow		OBJECT-TYPE
    SYNTAX		INTEGER
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "PDU Voltage LOW trap threshold x10."
    
    ::= { TRAPThresholds 2 }

