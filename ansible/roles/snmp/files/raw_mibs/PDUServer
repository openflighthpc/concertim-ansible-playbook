-- PDU.ENVIROSecurity

ENVIROSecurityDoor	OBJECT-TYPE
    SYNTAX			INTEGER {OPEN(0),CLOSED(1)}
    MAX-ACCESS	  	read-only
    STATUS  		current
    DESCRIPTION		"ENVIRO Module door contact status."
    
    ::= { ENVIROSecurity 1 }

ENVIROSecurityLock	OBJECT-TYPE
    SYNTAX			INTEGER {LOCKED(0),UNLOCKED(1)}
    MAX-ACCESS	  	read-write
    STATUS  		current
    DESCRIPTION		"ENVIRO Module lock status.
    Set with any data to force unlock."
    
    ::= { ENVIROSecurity 2 }

ENVIROSecurityWild	OBJECT-TYPE
    SYNTAX			INTEGER {DISABLED(0),ENABLED(1)}
    MAX-ACCESS	  	read-write
    STATUS  		current
    DESCRIPTION		"ENVIRO Module Card/PIN Wildcard status.
    Set with 1 to enable or 0 to disable Wildcard feature."
    
    ::= { ENVIROSecurity 3 }
  
ENVIROSecurityTable	OBJECT-TYPE
    SYNTAX			SEQUENCE OF ENVIROSecurityEntry
    MAX-ACCESS  		not-accessible
    STATUS  		current
    DESCRIPTION   	""
    
    ::= { ENVIROSecurity 4 }
    
ENVIROSecurityEntry	OBJECT-TYPE
    SYNTAX			ENVIROSecurityEntry
    MAX-ACCESS		not-accessible
    STATUS			current
    DESCRIPTION   	"Table of ENVIRO Card/PIN codes."
    INDEX			{ ENVIROSecurityIndex }    
    
    ::= { ENVIROSecurityTable 1 }

ENVIROSecurityEntry ::= SEQUENCE {

	  ENVIROSecurityIndex
		INTEGER,
	  ENVIROSecurityCode
		Integer32,
  }

ENVIROSecurityIndex	OBJECT-TYPE
    SYNTAX			INTEGER (1..32)
    MAX-ACCESS  		not-accessible
    STATUS  		current
    DESCRIPTION   	"Card/PIN code index."
    
    ::= { ENVIROSecurityEntry 1 }

ENVIROSecurityCode	OBJECT-TYPE
    SYNTAX			Integer32
    MAX-ACCESS	  	read-write
    STATUS  		current
    DESCRIPTION		"ENVIRO Module Card/PIN number."
    
    ::= { ENVIROSecurityEntry 2 }


-- PDUTraps.TRAPNotifications

PDUVRMSLow			NOTIFICATION-TYPE
    OBJECTS			{ SysName, SysLocation, PDUName, PDUStatusVRMS }
    STATUS current
    DESCRIPTION         "Voltage LOW trap notification"
    
    ::= { TRAPNotifications 1 }

PDUVRMSHigh		NOTIFICATION-TYPE
    OBJECTS			{ SysName, SysLocation, PDUName, PDUStatusVRMS }
    STATUS current
    DESCRIPTION         "Voltage HIGH trap notification"
    
    ::= { TRAPNotifications 2 }

PDUIRMSLow			NOTIFICATION-TYPE
    OBJECTS			{ SysName, SysLocation, PDUName, PDUStatusIRMS }
    STATUS current
    DESCRIPTION         "Current LOW trap notification"
    
    ::= { TRAPNotifications 3 }

PDUIRMSHigh		NOTIFICATION-TYPE
    OBJECTS			{ SysName, SysLocation, PDUName, PDUStatusIRMS }
    STATUS current
    DESCRIPTION         "Current HIGH trap notification"
    
    ::= { TRAPNotifications 4 }

