PDUServer DEFINITIONS ::= BEGIN

IMPORTS
   enterprises, IpAddress, Gauge, TimeTicks     FROM RFC1155-SMI
   DisplayString                                FROM RFC1213-MIB
   OBJECT-TYPE                                  FROM RFC-1212
   TRAP-TYPE                                    FROM RFC-1215
   MODULE-IDENTITY					FROM SNMPv2-SMI
   Integer32, Unsigned32			      FROM SNMPv2-SMI
   OBJECT-GROUP						FROM SNMPv2-CONF
   NOTIFICATION-GROUP			 		FROM SNMPv2-CONF
   MODULE-COMPLIANCE					FROM SNMPv2-CONF
   DateAndTime						FROM SNMPv2-TC
   TEXTUAL-CONVENTION					FROM SNMPv2-TC;

PDUServer			MODULE-IDENTITY

	LAST-UPDATED	"201108241712Z"
	ORGANIZATION	"Telecom Protection Technologies Ltd"
	CONTACT-INFO  	"sales@tpt.uk.com"
	DESCRIPTION		"TPT PDUServer MIB definition"

-- V1.00.00	20/06/2011
	REVISION		"201106201712Z"
	DESCRIPTION		"First release (without traps)"

-- V1.01.00	12/08/2011
	REVISION		"201108121712Z"
	DESCRIPTION		"Second release (with traps)"
	
-- V1.01.01	24/08/2011
	REVISION		"201108241712Z"
	DESCRIPTION		"Third release (Added OID's for PDU reset,
				KWH reset, LCD brightness & Client/Location under
				Configuration)"

-- V1.01.02	21/09/2011
	REVISION		"201109211712Z"
	DESCRIPTION		"Fourth release (Added kW register and kW trap
				 notification and threshold definitions)"
	
-- V1.01.03	9/11/2011
	REVISION		"201111091712Z"
	DESCRIPTION		"Fifth release (ASN.1 compatibility update)"
	
-- V1.02.00	22/12/2011
	REVISION		"201112221712Z"
	DESCRIPTION		"Sixth release (Added global PDU table to allow
				 access to all PDU data in system. Added IP Address,
				 Sub-net mask, DHCP configuration and alarm period.)"

-- V1.03.00	20/6/2012
	REVISION		"201206202012Z"
	DESCRIPTION		"Seventh release (Added global port status OID)"

-- V1.03.01	28/8/2012
	REVISION		"201208282012Z"
	DESCRIPTION		"Eighth release (Added port selection register)"

-- V1.04.00	27/9/2012
	REVISION		"201209272012Z"
	DESCRIPTION		"Ninth release (Added global registers for port amps and kWh's)"

	::= { TPTPDUServer 1 }

TPTPDUServer				OBJECT IDENTIFIER ::=  { enterprises 38218 }

PDUServer					OBJECT IDENTIFIER ::=  { TPTPDUServer 1 }
SiteTotals					OBJECT IDENTIFIER ::=  { PDUServer 1 }
Status					OBJECT IDENTIFIER ::=  { PDUServer 2 }
Configuration				OBJECT IDENTIFIER ::=  { PDUServer 3 }
TRAPDestinations				OBJECT IDENTIFIER ::=  { PDUServer 4 }

PDU						OBJECT IDENTIFIER ::=  { TPTPDUServer 2 }
PDUInfo					OBJECT IDENTIFIER ::=  { PDU 2 }
PMUStatus                          	OBJECT IDENTIFIER ::=  { PDU 3 }
PORTStatus                         	OBJECT IDENTIFIER ::=  { PDU 4 }
ENVIROStatus                      	OBJECT IDENTIFIER ::=  { PDU 5 }
ENVIROSecurity                     	OBJECT IDENTIFIER ::=  { PDU 6 }
PDUTraps					OBJECT IDENTIFIER ::=  { PDU 7 }
TRAPNotifications				OBJECT IDENTIFIER ::=  { PDUTraps 1 }
TRAPThresholds				OBJECT IDENTIFIER ::=  { PDUTraps 2 }

PDUGlobal					OBJECT IDENTIFIER ::=  { TPTPDUServer 3 }


-- PDUServer.SiteTotals

SiteTotalkWh	OBJECT-TYPE
    SYNTAX		Integer32
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "Total Energy used on site in kWh x10."
    
    ::= { SiteTotals 1 }

SiteTotalBtu	OBJECT-TYPE
    SYNTAX		Integer32
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "Total Energy used on site in Btu x10."
    
    ::= { SiteTotals 2 }

SiteTotalKgCO2	OBJECT-TYPE
    SYNTAX		Integer32
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "Total Energy used on site in KgCO2 x10."
    
    ::= { SiteTotals 3 }

-- PDUServer.Status

NumberOfPDUs	OBJECT-TYPE
    SYNTAX		INTEGER (0..32)
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "Number of PDU's connected to the PDUServer"
    
    ::= { Status 1 }

NumberOnline	OBJECT-TYPE
    SYNTAX		INTEGER (0..32)
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "Number of PDU's ONLINE."
    
    ::= { Status 2 }

ServerStatus	OBJECT-TYPE
    SYNTAX		INTEGER {STOPPED(0), RUNNING(1), SEARCHING(2), QUERYING(3)} 
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "Current PDUServer operational status."
    
    ::= { Status 3 }

-- PDUServer.Configuration

ClientName		OBJECT-TYPE
    SYNTAX		DisplayString
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "PDUServer Client name."
    
    ::= { Configuration 1 }

LocationName	OBJECT-TYPE
    SYNTAX		DisplayString
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "PDUServer Location Name."
    
    ::= { Configuration 2 }

FirmwareVersion	OBJECT-TYPE
    SYNTAX		DisplayString
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "PDUServer firmware version."
    
    ::= { Configuration 3 }

KgCO2Conversion	OBJECT-TYPE
    SYNTAX		INTEGER (1..1000)
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "KgCO2 conversion factor x1000 [544 = 0.544]"
    
    ::= { Configuration 4 }

MODBUSTimeout	OBJECT-TYPE
    SYNTAX		INTEGER (500..5000)
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "MODBUS Timeout in mS."
    
    ::= { Configuration 5 }

MODBUSRetry		OBJECT-TYPE
    SYNTAX		INTEGER (1..10)
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "MODBUS Retry value."
    
    ::= { Configuration 6 }

LCDBrightness	OBJECT-TYPE
    SYNTAX		INTEGER (0..1000)
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "LCD Backlight brightness."
    
    ::= { Configuration 7 }

AlarmPeriod		OBJECT-TYPE
    SYNTAX		INTEGER (10..1000)
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Period between alarm queries in seconds."
    
    ::= { Configuration 8 }

DHCPStatus		OBJECT-TYPE
    SYNTAX		INTEGER {DISABLED(0),ENABLED(1)}
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "DHCP Enable.Set to 1 to ENABLE or 0 to DISABLE DHCP support."
    
    ::= { Configuration 9 }

IPAddress		OBJECT-TYPE
    SYNTAX		Ipaddress
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "IP Address of PDU Server."
    
    ::= { Configuration 10 }

SubNetMask		OBJECT-TYPE
    SYNTAX		Ipaddress
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "PDU Server Sub-net mask."
    
    ::= { Configuration 11 }

-- PDUServer.TRAPDestinations

TRAPDestinationsTable	OBJECT-TYPE
    SYNTAX			SEQUENCE OF TRAPDestinationsEntry
    MAX-ACCESS  		not-accessible
    STATUS  		current
    DESCRIPTION   	""
    
    ::= { TRAPDestinations 1 }

TRAPDestinationsEntry	OBJECT-TYPE
    SYNTAX			TRAPDestinationsEntry
    MAX-ACCESS		not-accessible
    STATUS			current
    DESCRIPTION   	"Table of Trap notification destinations."
    INDEX			{ TRAPDestinationsIndex }    
    
    ::= { TRAPDestinationsTable 1 }

TRAPDestinationsEntry ::= SEQUENCE {

	  TRAPDestinationsIndex
		INTEGER,
	  TRAPDestinationsStatus
		INTEGER,
	  TRAPDestinationsIP
		Ipaddress,
	  TRAPDestinationsCommunity
		DisplayString,
  }

TRAPDestinationsIndex	OBJECT-TYPE
    SYNTAX			INTEGER (0..1)
    MAX-ACCESS  		not-accessible
    STATUS  		current
    DESCRIPTION   	"Trap destination index [0..1]."
    
    ::= { TRAPDestinationsEntry 1 }

TRAPDestinationsStatus	OBJECT-TYPE
    SYNTAX			INTEGER {DISABLED(0),ENABLED(1)}
    MAX-ACCESS	  	read-write
    STATUS  		current
    DESCRIPTION		"Trap destination status. Set to 1 to ENABLE or 0 to DISABLE trap destination."
    
    ::= { TRAPDestinationsEntry 2 }

TRAPDestinationsIP	OBJECT-TYPE
    SYNTAX			Ipaddress
    MAX-ACCESS	  	read-write
    STATUS  		current
    DESCRIPTION		"Trap destination IP address. Set to the IP address of the destination SNMP manager."
    
    ::= { TRAPDestinationsEntry 3 }

TRAPDestinationsCommunity	OBJECT-TYPE
    SYNTAX			DisplayString
    MAX-ACCESS	  	read-write
    STATUS  		current
    DESCRIPTION		"Trap destination SNMP community. Set to the community name to be used with the destination SNMP manager."
    
    ::= { TRAPDestinationsEntry 4 }


-- PDU

PDUSelected	OBJECT-TYPE
    SYNTAX		INTEGER (1..32)
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Currently selected PDU slot. Set to number between 1 and NumberOfPDUs"
    
    ::= { PDU 1 }

-- PDU.PDUInfo

PDUName		OBJECT-TYPE
    SYNTAX		DisplayString (SIZE (0..20))
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "PDU friendly name."
    
    ::= { PDUInfo 1 }

PDUStatus		OBJECT-TYPE
    SYNTAX		INTEGER {OFFLINE(0),ONLINE(1),DISABLED(2)}
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "PDU ONLINE / OFFLINE or DISABLED status."
    
    ::= { PDUInfo 2 }

PDUAddress		OBJECT-TYPE
    SYNTAX		INTEGER (1..32)
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "PDU MODBUS Address."
    
    ::= { PDUInfo 3 }

