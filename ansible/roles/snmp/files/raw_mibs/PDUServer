    OBJECTS			{ SysName, SysLocation, PDUName, PORTNumber }
    STATUS current
    DESCRIPTION         "Port Current HIGH trap notification"
    
    ::= { TRAPNotifications 10 }

ENVIROTemp1Low		NOTIFICATION-TYPE
    OBJECTS             { SysName, SysLocation, PDUName, ENVIROStatusTemp1 }
