-- PDUGlobal

PDUGlobalTable		OBJECT-TYPE
    SYNTAX			SEQUENCE OF PDUGlobalEntry
    MAX-ACCESS  		not-accessible
    STATUS  		current
    DESCRIPTION   	""
    
    ::= { PDUGlobal 1 }

PDUGlobalEntry		OBJECT-TYPE
    SYNTAX			PDUGlobalEntry
    MAX-ACCESS		not-accessible
    STATUS			current
    DESCRIPTION   	"Table of Trap notification destinations."
    INDEX			{ PDUGlobalIndex }    
    
    ::= { PDUGlobalTable 1 }

