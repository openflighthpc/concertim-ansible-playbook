PDUGlobalIndex		OBJECT-TYPE
    SYNTAX			INTEGER (0..31)
    MAX-ACCESS  		not-accessible
    STATUS  		current
    DESCRIPTION   	"PDU index [0..NoPDUs]."
    
    ::= { PDUGlobalEntry 1 }

PDUGlobalStatus		OBJECT-TYPE
    SYNTAX		INTEGER {OFFLINE(0),ONLINE(1),DISABLED(2)}
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "PDU ONLINE / OFFLINE or DISABLED status."
    
    ::= { PDUGlobalEntry 2 }

PDUGlobalAddress		OBJECT-TYPE
    SYNTAX		INTEGER (1..32)
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "PDU MODBUS Address."
    
    ::= { PDUGlobalEntry 3 }

