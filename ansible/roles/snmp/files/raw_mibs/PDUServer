    OBJECTS			{ SysName, SysLocation, PDUName, PDUStatusKW }
    STATUS current
    DESCRIPTION         "Kilowatts HIGH trap notification"
    
    ::= { TRAPNotifications 6 }

PDUTempLow			NOTIFICATION-TYPE
    OBJECTS			{ SysName, SysLocation, PDUName, PDUStatusTPMU }
    STATUS current
    DESCRIPTION         "PDU Temperature LOW trap notification"
    
    ::= { TRAPNotifications 7 }

PDUTempHigh		NOTIFICATION-TYPE
    OBJECTS			{ SysName, SysLocation, PDUName, PDUStatusTPMU }
    STATUS current
    DESCRIPTION         "PDU Temperature HIGH trap notification"
    
    ::= { TRAPNotifications 8 }

PORTAMPSLow		NOTIFICATION-TYPE
    OBJECTS			{ SysName, SysLocation, PDUName, PORTNumber }
    STATUS current
    DESCRIPTION         "Port Current LOW trap notification"
    
