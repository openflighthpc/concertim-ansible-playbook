    SYNTAX		DisplayString
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "PDU Firmware version."
    
    ::= { PDUInfo 4 }

PDUSerial		OBJECT-TYPE
    SYNTAX		DisplayString
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "PDU Serial number."
    
    ::= { PDUInfo 5 }

PDUNoPORTs		OBJECT-TYPE
    SYNTAX		INTEGER (0..32)
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "PDU Number of ports."
    
    ::= { PDUInfo 6 }

PDUAllON		OBJECT-TYPE
    SYNTAX		INTEGER
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Set with any value to turn ALL ports ON."
    
    ::= { PDUInfo 7 }

PDUAllOFF		OBJECT-TYPE
    SYNTAX		INTEGER
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Set with any value to turn ALL ports OFF."
    
    ::= { PDUInfo 8 }

PDUReset		OBJECT-TYPE
    SYNTAX		INTEGER
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Set with any value to RESET PDU."
    
    ::= { PDUInfo 9 }

PDUKWHReset	OBJECT-TYPE
    SYNTAX		INTEGER
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Set with any value to reset kWh value."
    
    ::= { PDUInfo 10 }

