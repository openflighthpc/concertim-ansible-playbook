    SYNTAX		INTEGER {OFFLINE(0),ONLINE(1),DISABLED(2)}
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Compatible - PDU ONLINE / OFFLINE or DISABLED status."
    
    ::= { Slave-Global-Entry 2 }

