    SYNTAX		INTEGER (0)
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "Compatible - Not used."
    
    ::= { PDU-Configuration 7 }

PDU-AlarmPeriod	OBJECT-TYPE
    SYNTAX		INTEGER (1..240)
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Compatible - Alarm period check in minutes."
    
    ::= { PDU-Configuration 8 }

PDU-DHCPStatus	OBJECT-TYPE
    SYNTAX		INTEGER {DISABLED(0),ENABLED(1)}
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Compatible - DHCP Enable.Set to 1 to ENABLE or 0 to DISABLE DHCP support."
    
