-- PDU.Slave-Sockets

Slave-SocketsTable	OBJECT-TYPE
    SYNTAX		SEQUENCE OF PDUStatusEntry
    MAX-ACCESS  	not-accessible
    STATUS  	current
    DESCRIPTION   ""
    
    ::= { Slave-Sockets 1 }
    
Slave-SocketsEntry	OBJECT-TYPE
    SYNTAX		Slave-SocketsEntry
    MAX-ACCESS	not-accessible
    STATUS		current
    DESCRIPTION   "Table of Socket status parameters."
    INDEX		{ SocketIndex }    
    
    ::= { Slave-SocketsTable 1 }

Slave-SocketsEntry ::= SEQUENCE {

	  Socket-Index
		INTEGER,
	  Socket-Number
		INTEGER,
	  Socket-NotUsed1
		DisplayString,
	  Socket-ONOFF
		INTEGER,
	  Socket-AMPS
		INTEGER,
	  Socket-KWH
		Integer32,
  }

Socket-Index	OBJECT-TYPE
    SYNTAX		INTEGER
    MAX-ACCESS  	not-accessible
    STATUS  	current
    DESCRIPTION   "Socket index"
    
    ::= { Slave-SocketsEntry 1 }

Socket-Number	OBJECT-TYPE
    SYNTAX		INTEGER
    MAX-ACCESS  	not-accessible
    STATUS  	current
    DESCRIPTION   "Socket number [Range 1 to 48]"
    
    ::= { Slave-SocketsEntry 2 }

Socket-NotUsed1		OBJECT-TYPE
    SYNTAX		DisplayString
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "Compatible - Not used."
    
    ::= { Slave-SocketsEntry 3 }

