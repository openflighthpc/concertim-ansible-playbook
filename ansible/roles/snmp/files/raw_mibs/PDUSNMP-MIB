    SYNTAX		DisplayString
    MAX-ACCESS  	read-only
    STATUS  	current
    DESCRIPTION   "Compatible - PDU firmware version."
    
    ::= { PDU-Configuration 3 }

PDU-KgCO2Conversion	OBJECT-TYPE
    SYNTAX		INTEGER (1..1000)
    MAX-ACCESS  	read-write
    STATUS  	current
    DESCRIPTION   "Compatible - KgCO2 conversion factor x1000 [544 = 0.544]"
    
    ::= { PDU-Configuration 4 }

