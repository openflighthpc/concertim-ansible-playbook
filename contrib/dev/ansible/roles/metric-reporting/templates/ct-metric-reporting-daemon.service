[Unit]
Description=Concertim Metric Reporting Daemon - Provides HTTP API to allow injecting custom metrics into the metric stream.

[Service]
Type=exec
ExecStart={{ct_installation_dir}}/{{daemon}}/{{daemon}}
TimeoutSec=10sec
StandardOutput=append:/var/log/{{daemon}}.log
StandardError=append:/var/log/{{daemon}}.log

[Install]
WantedBy=multi-user.target
