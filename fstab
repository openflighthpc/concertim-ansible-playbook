# /etc/fstab: static file system information.
#
# <file system> <mount point>   <type>  <options>       <dump>  <pass>
proc            /proc           proc    defaults        0       0

LABEL=_boot		/boot		ext3e	defaults	0 0
LABEL=_		/		ext4		defaults		0 0
LABEL=_upgrade		/upgrade		ext4		defaults		0 0
LABEL=SWAP-sda5		none		swap		defaults		0 0
LABEL=_data_private		/data/private		ext4		defaults		0 0
LABEL=_data_upgrade		/data/upgrade		ext4		defaults		0 0
LABEL=_data_public		/data/public		ext4		defaults		0 0

/data/public/resources		/resources		none		bind		0 0
tmpfs /tmp tmpfs defaults 0 2
# /data/private/share/snmp/mibs is an NFS mount from command over the VPN ip.
# The mount is set up manually in the /usr/local/sbin/vpn-ip.rb file
# (located within the network-services bundle)
/data/public/resources/tftpboot		/var/lib/tftpboot/local		none		bind		0 0
tmpfs /var/lib/ganglia/rrds tmpfs uid=ganglia,gid=ganglia,mode=755 0 0
/data/public/safe		/safe		none		bind		0 0
/data/private/share/rails		/opt/concurrent-thinking		none		bind		0 0
